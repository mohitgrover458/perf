            <stringProp name="script">import java.io.*;

int matches = Integer.parseInt(vars.get(&quot;atlasIDPracticeQuiz_matchNr&quot;));
int chmatches = Integer.parseInt(vars.get(&quot;chapterPracticeQuiz_matchNr&quot;));
Set tagColl = new HashSet();
Set chapterColl = new HashSet();

for(int i = 1; i &lt;= matches; i++) {

  tagColl.add(vars.get(&quot;atlasIDPracticeQuiz_&quot; + i));
  vars.remove(&quot;atlasIDPracticeQuiz_&quot; + i);
}

int counter = 1;
for (String tag : tagColl) {
  vars.put(&quot;atlasIDPracticeQuiz_&quot; + counter, tag);
  counter++;
}

for(int j=1;j &lt;= chmatches; j++) {
	chapterColl.add(vars.get(&quot;chapterPracticeQuiz_&quot; + j));
	vars.remove(&quot;chapterPracticeQuiz_&quot; + j);
}

int chcounter = 1;
for (String chapter : chapterColl) {
	vars.put(&quot;chapterPracticeQuiz_&quot; + chcounter, chapter);
	chcounter++;
}

String stringPracticeQuizSB = &quot;&quot;;

for(int i=1; i&lt;=tagColl.size(); i++) {
	if(i == 1) {
		stringPracticeQuizSB += &quot;&quot;+vars.get(&quot;atlasIDPracticeQuiz_&quot;+i);
	} else {
		stringPracticeQuizSB += &quot;,&quot;+vars.get(&quot;atlasIDPracticeQuiz_&quot;+i);
